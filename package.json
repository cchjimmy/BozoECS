{
	"name": "bozoecs",
	"version": "0.0.29",
	"description": "entity component system",
	"type": "module",
	"types": "types",
	"scripts": {
		"start": "npm run build; npm run serve",
		"dev": "nodemon --exec 'npm run build-debug; npm run serve' --watch examples --ignore **/build/** -e ts",
		"serve": "esbuild --servedir=examples",
		"build": "node --experimental-strip-types build.ts",
		"build-debug": "npm run build -- debug"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cchjimmy/BozoECS.git"
	},
	"keywords": [
		"ecs"
	],
	"author": "cchjimmy",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/cchjimmy/BozoECS/issues"
	},
	"homepage": "https://github.com/cchjimmy/BozoECS#readme",
	"devDependencies": {
		"esbuild": "^0.25.4",
		"nodemon": "^3.1.10"
	}
}
